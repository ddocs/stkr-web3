    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@polkadot/react-hooks": "^0.39.1",
    "@redux-requests/axios": "^1.1.1",
    "@redux-requests/core": "1.6.1",
    "@redux-requests/promise": "1.0.2",
