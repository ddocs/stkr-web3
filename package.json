    "@types/react": "^16.9.0",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^16.9.0",
    "@walletconnect/web3-provider": "^1.6.5",
    "axios": "^0.20.0",
    "bignumber.js": "^9.0.1",
    "connected-react-router": "^6.8.0",
    "date-fns": "^2.16.1",
    "ethereumjs-tx": "^2.1.2",
    "events": "^3.2.0",
    "final-form": "^4.20.1",
    "fontfaceonload": "^1.0.2",
    "jss-preset-default": "^10.7.1",
    "puppeteer": "^10.1.0",
    "react": "^16.13.1",
    "react-animated-numbers": "0.3.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-intersection-observer": "^8.32.0",
    "react-intl-universal": "^2.4.1",
    "react-player": "^2.7.2",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "react-shallow-context": "^1.0.1",
    "react-snap": "^1.23.0",
    "redux": "^4.0.5",
    "redux-action": "^1.2.2",
    "redux-actions": "^2.6.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-smart-actions": "^1.0.0",
    "reselect": "^4.0.0",
    "typescript": "~3.7.2",
    "web3": "1.2.11",
    "web3modal": "^1.9.4"
  },
  "devDependencies": {
    "@storybook/cli": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^5.3.19",
    "@types/classnames": "^2.2.10",
    "@types/ethereumjs-tx": "^2.0.0",
    "@types/loadable__component": "^5.13.1",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-actions": "^2.6.1",
    "@types/web3": "^1.2.2",
    "babel-loader": "8.1.0",
    "classnames": "^2.2.6",
    "env-cmd": "^10.1.0",
    "history": "^4.10.1",
    "husky": "^4.3.0",
    "intersection-observer": "^0.11.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "react-app-rewired": "^2.1.6",
    "react-final-form": "^6.5.1",
    "react-focus-on": "^3.5.0",
    "react-stroller": "^1.6.7",
    "react-uid": "^2.3.0",
    "resolve-from": "^5.0.0",
    "storybook": "^5.3.19"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "skipThirdPartyRequests": true,
    "inlineCss": true
  },
  "scripts": {
    "start:develop": "env-cmd -f .env.develop react-scripts start",
    "start:stage": "env-cmd -f .env.stage react-scripts start",
    "start:prod": "env-cmd -f .env.prod react-scripts start",
    "build": "env-cmd react-scripts build",
    "test": "env-cmd react-scripts test",
    "eject": "env-cmd react-scripts eject",
    "storybook": "env-cmd start-storybook",
    "postbuild": "react-snap"
  },
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "react-app"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "react-app"
    ],
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/ban-types": [
            "error",
            {
              "types": {
                "React.FC": {
                  "message": "In favor of unification use the form \"(props: IComponentNameProp) => ReactNode\""
                }
              }
            }
          ],
          "@typescript-eslint/ban-ts-ignore": "off",
          "@typescript-eslint/no-empty-interface": "off",
          "@typescript-eslint/explicit-function-return-type": "off",
          "@typescript-eslint/no-explicit-any": "off",
          "@typescript-eslint/camelcase": "off",
          "@typescript-eslint/explicit-module-boundary-types": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
